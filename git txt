./code-generator/generate-groups.sh all Kubewatch/pkg/client Kubewatch/pkg/apis math:v1alpha1 --go-header-file code-generator/hack/boilerplate.go.txt --output-base ..


curl -L -o kubebuilder https://go.kubebuilder.io/dl/latest/$(go env GOOS)/$(go env GOARCH)
